{
  "name": "vite-vue-ssr-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node-esm server.ts",
    "build": "npm run build:client && npm run build:server && npm run build:express",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "build:express": "tsc --p ./tsconfig-express.json",
    "preview": "vite preview",
    "serve": "cross-env NODE_ENV=production node dist/express/server"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
